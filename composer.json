{
  "name": "julienmru/laravel-paybox-gateway",
  "description": "Laravel Paybox gateway",
  "version": "1.5.2",
  "require": {
    "laravel/framework": ">=5.5",
    "guzzlehttp/guzzle": ">=6.2",
    "ext-openssl": "*",
    "ext-iconv": "*",
    "codeinc/strip-accents": "^1.1",
    "spatie/array-to-xml": "^2.15"
  },
  "autoload": {
    "psr-4": {
      "JulienMru\\PayboxGateway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "license": "MIT",  
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^5.4",
    "mockery/mockery": "^0.9.5"
  }
}
